{
  "name": "focus-sgrh",
  "version": "1.0.0",
  "description": "API do Sistema de Gest√£o de Recursos Humanos - SGRH",
  "author": "Focus Consultoria",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/src/server.js",
    "dev": "nodemon -L ",
    "build": "tsc -p tsconfig-build.json",
    "test": "npm run lint & jest --verbose --coverage",
    "test:watch": "jest --verbose --watchAll",
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "npm run lint -- --fix",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/sgrh/config/database.ts migration:generate ./migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/sgrh/config/database migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/sgrh/config/database migration:revert",
    "prepare": "husky install",
    "docs": "typedoc --tsconfig tsconfig.json"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "jest",
    "postgres",
    "pattern"
  ],
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "faker": "^6.6.6",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.4.0"
  }
}
